<% if(success && success.length){ %>
    <div class="alert alert-success alert-dismissible fade show col-6 offset-3 mt-1" role="alert">
        <%=success%>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>   
<%}%>

<%if(deletemsg && deletemsg.length){%>
    <div class="alert alert-danger alert-dismissible fade show col-6 offset-3 mt-1" role="alert">
        <%=deletemsg%>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%>

<%if(updatemsg && updatemsg.length){%>
    <div class="alert alert-primary alert-dismissible fade show col-6 offset-3 mt-1" role="alert">
        <%=updatemsg%>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%>

<%if(errormsg && errormsg.length){%>
    <div class="alert alert-warning alert-dismissible fade show col-6 offset-3 mt-1" role="alert">
        <%=errormsg%>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%>